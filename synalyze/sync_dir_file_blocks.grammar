            <number name="type" id="55" type="integer" length="32" lengthunit="bit"/>
            <number name="block_size" id="56" type="integer" length="32" lengthunit="bit"/>
            <number name="size" id="57" type="integer" length="64" lengthunit="bit"/>
            <number name="uid" id="58" type="integer" length="32" lengthunit="bit"/>
            <number name="gid" id="59" type="integer" length="32" lengthunit="bit"/>
            <structref name="atimespec" id="61" fillcolor="FF682E" structure="id:60"/>
