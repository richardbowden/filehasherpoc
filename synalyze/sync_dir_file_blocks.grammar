        <structure name="timespec" id="60" length="0" encoding="ISO_8859-1:1987" endian="little" signed="no">
            <number name="tv_sec" mustmatch="yes" id="72" type="integer" length="8" endian="little"/>
            <number name="tv_nsec" mustmatch="yes" id="73" type="integer" length="8" endian="little"/>
        </structure>
    </grammar>
</ufwb>
